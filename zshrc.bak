export EDITOR=nvim

function db() {
if [ -n "$1" ]; then
  local_exists=`git show-ref refs/heads/$1`
  remote_exists=`git ls-remote origin $1`
  if [ -n "$local_exists" ]; then
    git branch -D $1;
  else
    echo "could not find local branch: $1";
  fi
  if [ -n "$remote_exists" ]; then
    git ps origin :$1;
  else
    echo "could not find remote branch: $1";
  fi
fi
}

compdef _git db=git-branch

eval "$(hub alias -s)"
